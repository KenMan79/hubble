<%= render partial: '/tezos/shared/network_header', locals: { active_tab: 'Summary', summary_link: true, governance_link: true } %>

<div class='container'>
  <div class='content'>
    <% if @search %>

      <%= render "search_results" %>

    <% else %>

      <%= render "header" %>
      <%= render "donut_charts" %>
      <%= render "start_and_end" %>
